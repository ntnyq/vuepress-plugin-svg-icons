{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "vue",
    "html",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.words": [
    "dbaeumer",
    "esbenp",
    "pathe",
    "pnpm",
    "shiki",
    "svgs",
    "tsbuildinfo",
    "vite",
    "vuepress",
    "xlink"
  ],
  "cSpell.allowCompoundWords": true
}
