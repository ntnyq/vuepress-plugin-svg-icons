{
  "name": "@goy/vuepress-plugin-svg-icons",
  "version": "4.2.3",
  "description": "SVG sprite icon plugin for VuePress",
  "keywords": [
    "vuepress-plugin",
    "vuepress",
    "svg-sprite-icon",
    "svg-sprite",
    "iconfont"
  ],
  "homepage": "https://vp-icon.goyfe.com",
  "bugs": {
    "url": "https://github.com/ntnyq/vuepress-plugin-svg-icons/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/vuepress-plugin-svg-icons.git"
  },
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "copy": "copyfiles -a -u 1 -V src/*.{styl,js} lib",
    "dev": "run-s clean dev:lib",
    "build": "run-s clean build:lib copy",
    "dev:lib": "tsc-watch --onSuccess \"npm run copy\"",
    "build:lib": "tsc --build",
    "dev:docs": "npm -C docs run dev",
    "build:docs": "npm -C docs run build",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "lint-staged": {
    "*.{yml,md}": "prettier --write"
  },
  "dependencies": {
    "svg-mixer": "^2.3.14",
    "svgo": "^2.8.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "devDependencies": {
    "@ntnyq/prettier-config": "^0.3.1",
    "copyfiles": "^2.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tsc-watch": "^5.0.2",
    "typescript": "^4.6.3",
    "vue": "^2.6.14",
    "vuepress": "^1.9.7",
    "vuepress-types": "^0.9.4"
  },
  "peerDependencies": {
    "vuepress": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "css-loader"
      ]
    }
  }
}
